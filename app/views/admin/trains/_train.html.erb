	<tr>
		<td><%= link_to train.number, [:admin, train] %></td>
		<td><% if train.current_station.present? %>
		<%= train.current_station.title %>
		<% else %>
		Станция не указана
		<% end %></td>
		<td><%= link_to 'Показать', [:admin, train] %></td>
		<td><%= link_to 'Редактировать', edit_admin_train_path(train)%></td>
		<td><%= link_to 'Удалить',[:admin, train], method: :delete, data: {confirm: 'Уверены?'} %></td>
	</tr>